public class TypeCastingExample {
    public static void main(String[] args) {
        double doubleVal = 9.78;
        int intVal = (int) doubleVal; // Narrowing casting

        System.out.println("Double value: " + doubleVal);
        System.out.println("Converted to int: " + intVal);

        int intNum = 10;
        double convertedDouble = intNum; // Widening casting

        System.out.println("Int value: " + intNum);
        System.out.println("Converted to double: " + convertedDouble);
    }
}
